<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="zzxb.me.layoutdemo">
    <uses-permission
        android:name="android.permission.VIBRATE"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".LinearActivity" />
        <activity android:name=".TableLayoutActivity" />
        <activity android:name=".FrameActivity" />
        <activity android:name=".RelativeActivity" />
        <activity android:name=".AbsLayoutActivity" />
        <activity
            android:name=".IndexActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_index"
            android:theme="@style/FullscreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>